<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<fx:root styleClass="taskNode" type="AnchorPane"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1">
	<stylesheets>
		<URL value="@tasknode.css" />
	</stylesheets>
	<children>
		<StackPane AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label fx:id="taskName" alignment="CENTER"
					contentDisplay="CENTER" text="Label" textAlignment="CENTER" />
			</children>
		</StackPane>
		<AnchorPane fx:id="subTasks"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
		</AnchorPane>
	</children>
</fx:root>
